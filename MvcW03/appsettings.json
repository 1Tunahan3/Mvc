{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CookieAuthOptions": {
    "Name": "SecureToken",
    "LoginPath": "/Auth/Login",
    "LogoutPath": "/Auth/Logout",
    "AccessDeniedPath": "/Auth/Err",
    "SlidingExpiration": true,
    "Timeout": 600 

  },



  "AllowedHosts": "*"
}
